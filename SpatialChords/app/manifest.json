{
  "manifest_version": 2,

  "name": "Spatial Chords",
  "description": "This extension lets you navigate through focusable items using arrow keys.",
  "version": "0.1",
  "content_scripts": [ {
      "all_frames": true,
			"matches": ["<all_urls>"],
			"css": ["SpatialChords.css"],
			"js": ["jquery-2.1.3.min.js", "jquery.keynav.js", "SpatialChords.js"],
      "run_at": "document_end"
		} ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}